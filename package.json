{
  "name": "@tesseractcollective/react-graphql",
  "version": "0.9.12",
  "private": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm devmode",
    "build": "pnpm webmode && vite build && ttsc --project tsconfig-web.build.json",
    "tsc": "ttsc --project tsconfig-web.build.json",
    "pack": "pnpm pack",
    "pub": "pnpm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "webmode": "node ./scripts/enterwebmode.js",
    "devmode": "node ./scripts/enterdevmode.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx|js)": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.64.4",
    "@urql/storybook-addon": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "babel-loader": "^8.2.2",
    "enzyme": "^3.11.0",
    "esbuild": "^0.11.20",
    "esbuild-plugin-alias": "^0.1.2",
    "esbuild-plugin-flow": "^0.3.2",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.64.1",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3",
    "typescript-transform-paths": "^3.1.0",
    "urql": "^2.0.3",
    "vite": "^2.4.4",
    "vite-tsconfig-paths": "^2.3.0",
    "web-vitals": "^1.0.1"
  },
  "module": "dist/react-graphql.es.js",
  "types": "dist/index.d.ts",
  "main": "dist/react-graphql.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-graphql.es.js"
    }
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "urql": "^2.0.4"
  },
  "dependencies": {
    "case": "^1.6.3",
    "jotai": "^1.1.2",
    "lodash": "^4.17.21",
    "query-string": "^7.0.1",
    "type-fest": "^1.0.2",
    "wonka": "^4.0.15"
  }
}